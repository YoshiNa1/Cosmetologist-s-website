@model List<OOPLab13._1.Models.Client>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head runat="server">
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" type="text/css" href="~/css/main.css" />
    <title>Clients</title>
</head>
<body>
    <h2>Here is reserved time</h2>
    <table>
        <thead>
            <tr>
                <th>Date</th>
                <th>Time</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Client in Model)
            {
                <tr>
                    <td>@Client.Date</td>
                    <td>@Client.Time</td>
                    <td>@Client.Name</td>
                </tr>
            }
        </tbody>
    </table>
    <p> @Html.ActionLink("Go home", "Index")</p>
</body>
</html>